      "parameters": {
        "jsCode": "// Initialize counters\nlet created = 0;\nlet updated = 0;\nlet duplicates = 0;\nlet errors = 0;\n\n// Loop through all records from Airtable\nfor (const item of items) {\n  const outcome = item.json[\"Workflow Outcome\"];\n  \n  if (outcome === \"Created\") created++;\n  else if (outcome === \"Updated\") updated++;\n  else if (outcome === \"Duplicate\") duplicates++;\n  else if (outcome === \"Error\") errors++;\n}\n\n// Return summary object\nreturn [\n  {\n    json: {\n      total: items.length,\n      created,\n      updated,\n      duplicates,\n      errors\n    }\n  }\n];\n"
      },
